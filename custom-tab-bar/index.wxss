.selfTabBar {
  display: flex;
  height: 50px;
  background: #fff;
  align-items: center;
  justify-content: center;
  width: 100%;
}

.selfTabBar .cameraBarItem,
.selfTabBar .activeBar,
.selfTabBar .selfTabBarItem {
  display: flex;
  flex-direction: column;
  flex: 1;
  align-items: center;
  justify-content: center;
  color: rgba(0, 0, 0, 0.8);
  height: 100%;
}

.selfTabBar .cameraBarItem .tabTitle,
.selfTabBar .activeBar .tabTitle,
.selfTabBar .selfTabBarItem .tabTitle {
  font-size: 11px;
  margin-top: 4rpx;
}

.selfTabBar .activeBar {
  color: #bea070;
}

.selfTabBar .cameraBarItem {
  background: #bea070;
  border-radius: 4px;
  height: 36px;
  width: 80%;
  color: #fff;
  font-size: 24px;
  display: flex;
  align-items: center;
  justify-content: center;
}
